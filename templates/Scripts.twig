{% for script in scripts %}
	{% if script.inline %}
		<script type="{{ script.type }}">
			/* <![CDATA[ * /
			{{ script.content }}
			/* ]]> */
		</script>
	{% else %}
		<script type="{{ script.type }}" src="{{ script.url|raw }}" {%- if script.defer %} defer{% endif %}></script>
	{% endif %}
{% endfor %}
