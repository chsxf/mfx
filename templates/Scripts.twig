{% for script in scripts %}
	{% if script.inline %}
		<script type="{{ script.type }}">
			/* <![CDATA[ * /
			{{ script.content }}
			/* ]]> */
		</script>
	{% else %}
		<script type="{{ script.type }}" src="{{ script.url|raw }}" {% for extra_key, extra_value in script.extras %} {{ extra_key }}="{{ extra_value|e('html_attr') }}" {% endfor %}></script>
	{% endif %}
{% endfor %}
